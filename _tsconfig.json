      "./taktik_components/ozone-edit-video/type",
      "./taktik_components/ozone-video-player/type"
    ],
    "types": [
      "hls-parser"
    ],
    "declaration": true,
    /* Generates corresponding '.d.ts' file. */
    "rootDir": "./",
